#include <stdio.h>
int add(int);
int main()
{
    int x448, y448;
    printf("Enter the number : ");
    scanf("%d",&x448);
    y448 = add(x448);
    printf("Result is : %d",y448);
}
int add(int x448)
{
    if(x448==1)
    {
        return 1*5;
    }
    else
    {
        return (x448*5)+add(x448-1);
    }
}


