#include <stdio.h>

int main(void)
{
    int int448;
    float float448;
    double double448;
    char char448;
    
    printf("Size of int: %ld bytes\n",sizeof(int448));
    printf("Size of float: %ld bytes\n",sizeof(float448));
    printf("Size of double: %ld bytes\n",sizeof(double448));
    printf("Size of char: %ld byte\n",sizeof(char448));
    
    return 0;
}

