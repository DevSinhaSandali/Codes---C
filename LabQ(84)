#include <stdio.h>
#include <math.h>

int main()
{
    double x448,y448,X448,Y448,D448;
    printf("Enter coordinates of first point \n");
    scanf("%lf %lf",&x448,&y448);
    printf("Enter coordinates of second point \n");
    scanf("%lf %lf",&X448,&Y448);
    D448=sqrt(((X448-x448)*(X448-x448))+((Y448-y448)*(Y448-y448)));
    printf("Distance between the two points=%lf",D448);

    return 0;
}

