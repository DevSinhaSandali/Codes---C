#include <stdio.h>

int main(void)
{
    float a;
    float b;
    float c;
    printf("Enter value of a \n");
    scanf("%f",&a);
    printf("Enter value of b \n");
    scanf("%f",&b);
    printf("Enter value of c \n");
    scanf("%f",&c);
    
    float x= a/(b-c);
    
    printf("the value of x is %f\n",x);

    return 0;
}


