#include <stdio.h>
int main()
{
int arr1388[10][10],arr2388[10][10],arr3388[10][10];
int m388,n388;
printf(" Enter the row number");
scanf("%d",&m388);
printf("Enter the coloumn number");
scanf("%d",&n388);
for(int i =0;i<m388;i++){
for(int j =0;j<n388;j++){
printf("Enter[%d][%d]",i,j);
scanf("%d",&arr1388[i][j]);
}
}
for(int i =0;i<m388;i++){
for(int j =0 ;j<n388;j++){
printf("Enter[%d][%d]",i,j);
scanf("%d",&arr2388[i][j]);
}
}
for(int i =0;i<m388;i++){
for(int j =0;j<n388;j++){
arr3388[i][j]=arr1388[i][j]+arr2388[i][j];
}
}
for(int i=0;i<m388;i++){
printf("\n");
for(int j =0;j<n388;j++){
printf(" %d\t",arr3388[i][j]);
}
}

    return 0;
}


