#include <stdio.h>
#include<stdlib.h>
int main(void)
{
    double *ptr, sum = 0;
    double i;
    printf("ENTER THE NUMBER:-");
    ptr = (double *)calloc(5, sizeof(double));
    for (i = 0; i < 5; ++i)
    {
        scanf("%d", ptr);
        sum += *ptr;
    }

    printf("Sum = %d", sum);
    return 0;
}

