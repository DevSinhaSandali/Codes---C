#include <stdio.h>
#include <string.h>

int main()
{
    int b448,i=0;
    char str448[100];
    printf("Enter a STRING ");
    scanf("%s",str448);
    b448 = strlen(str448);
    for(i=0;i<b448;i++)
    {
        if(str448[i]>='a'&&str448[i]<='z')
        {
            str448[i]-=32;
    
         }
         else
    {
        str448[i]+=32;
    }
    }
    str448[i]='\0';
    printf("%s\n",str448);
    
    return 0;
}


